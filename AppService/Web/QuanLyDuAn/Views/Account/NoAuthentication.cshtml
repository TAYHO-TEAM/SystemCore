@{
    Layout = null;
    ViewBag.Title = "Giới hạn quyền truy cập";
    var version = System.Configuration.ConfigurationManager.AppSettings["webpages:Version"];
}
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Quản lý dự án - Tây Hồ Center</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/StyleBundle?v=" + version)
    @Scripts.Render("~/ScriptsBundle?v=" + version)
</head>
<body class="hold-transition sidebar-mini layout-fixed">

    <div class="wrapper">
        <div class="content-wrapper" style="margin-left:0px !important">
            <section class="content">
                <div class="container-fluid">
                    <section class="content">
                        <div class="error-page">
                            <h2 class="headline text-danger font-weight-bold">401</h2>
                            <div class="error-content">
                                <h3 class="font-weight-bold text-danger"><i class="fas fa-exclamation-triangle"></i> Giới hạn quyền truy cập.</h3>
                                <p>
                                    Tài khoản này không có quyền truy cập vào tính năng năng đã chọn<br />
                                    Vui lòng liên hệ <em class="font-weight-bolder text-tayho">Phòng Hệ thống</em> để được hỗ trợ thêm. Hoặc <a class="font-weight-bolder text-tayho" href="/Home">trở lại</a> màn hình chính.
                                </p>
                            </div>
                        </div>
                    </section>
                </div>
            </section>
        </div>
    </div>

</body>
</html>
